<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  		
<mapper namespace="dataReply">
	<select id="selectReplyCnt" parameterType="int" resultType="_int">
		SELECT COUNT(*)
		FROM DATA_REPLY
		WHERE B_NO = #{b_no}
	</select>
</mapper>
